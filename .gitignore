/.next/
/out/
.next/
out/

# production
/build
/dist

# misc
.DS_Store
*.pem
Thumbs.db
*.log

# debug
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*
lerna-debug.log*

# runtime data
pids/
*.pid
*.seed
