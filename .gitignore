# env files (can opt-in for committing if needed)
.env*
!.env.example
.env
.env.local
.env.development
.env.production
.env.test

# terraform secrets and sensitive files
# Terraform: ignore state, local cache, lockfiles, and variable files
# Keep examples (e.g. *.tfvars.example) by negating the example pattern below
*.tfvars
!*.tfvars.example
*.tfvars.json
*.tfstate*
.terraform/
.terraform.lock.hcl

# Also ignore Terraform files in service subfolders
go-services/*/terraform/secrets.tfvars
go-services/*/terraform/.terraform/
go-services/*/terraform/.terraform.lock.hcl
go-services/*/terraform/terraform.tfstate*
go-services/*/terraform/*.tfstate
go-services/*/terraform/*.tfstate.*

# service account credentials and API keys
go-services/*/keys/
*.json
!package.json
!tsconfig.json
!components.json
*.pem
*.key
*.crt

# Go specific
go-services/*/tmp/
go-services/*/temp/
go-services/*/*.log
go-services/*/vendor/
go-services/*/go.work
go-services/*/go.work.sum

# Executables / build artifacts
# ignore compiled windows binaries produced by local builds
/go-services/*.exe
/go-services/**/*.exe

# vercel
.vercel

# typescript
*.tsbuildinfo
next-env.d.ts

