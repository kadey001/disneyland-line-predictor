datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model RideWaitTimeSnapshot {
  id           Int      @id @default(autoincrement())
  rideId       Int
  rideName     String
  isOpen       Boolean
  waitTime     Int
  snapshotTime DateTime
}
